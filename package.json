{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --watch",
    "test-all": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "eslint-plugin-jsx-no-leaked-render",
  "author": "Grant Kiely",
  "module": "dist/eslint-plugin-jsx-no-leaked-render.esm.js",
  "size-limit": [
    {
      "path": "dist/eslint-plugin-jsx-no-leaked-render.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/eslint-plugin-jsx-no-leaked-render.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.0.1",
    "eslint": "^6.8.0",
    "husky": "^8.0.1",
    "size-limit": "^8.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2",
    "@typescript-eslint/parser": "^5.36.1"
  }
}
